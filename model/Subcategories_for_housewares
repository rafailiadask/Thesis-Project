package com.example.productspurchaseapp.Model;

public class Subcategories_for_housewares {
        private String pname,image,description;
        private int price;
        public Subcategories_for_housewares(){

        }
        public Subcategories_for_housewares(String pname,String description,String image,int price){
            this.pname=pname;
            this.description=description;
            this.image=image;
            this.price=price;
        }

        public String getPname() {
            return pname;
        }

        public void setPname(String pname) {
            this.pname = pname;
        }

        public String getImage() {
            return image;
        }

        public void setImage(String image) {
            this.image = image;
        }

        public String getDescription() {
            return description;
        }

        public void setDescription(String description) {
            this.description = description;
        }

        public int getPrice() {
            return price;
        }

        public void setPrice(int price) {
            this.price = price;
        }
    }

